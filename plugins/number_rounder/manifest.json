{
  "name": "number_rounder",
  "description": "Round a number to a fixed number of decimal places.",
  "input_schema": {
    "type": "object",
    "properties": {
      "value": {
        "type": "number"
      },
      "decimals": {
        "type": "integer",
        "minimum": 0,
        "maximum": 10
      }
    },
    "required": [
      "value"
    ]
  },
  "execution_function": "number_rounder"
}